<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Your Games</title>
</head>
<body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>Your Games</h1>
    <a href="/users/<%=user._id%>/games/new">Add a Game</a>
    <ul>
        <% games.forEach((game) => { %>
        <li><%= game.name %> on <%= game.console %></li>
        <% }) %>
    </ul>
</body>
</html>